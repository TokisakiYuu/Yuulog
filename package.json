{
    "name": "yuulog",
    "version": "0.1.0",
    "description": "best blog system for myself",
    "repository": {
        "type": "git",
        "url": "https://github.com/TokisakiYuu/Yuulog.git"
    },
    "author": "Tokisaki Yuu <tokisakiyuu@qq.com>",
    "license": "MIT",
    "scripts": {
        "build": "npm run build-ts && npm run lint",
        "build-frontend": "ts-node scripts/build-frontend.ts",
        "build-ts": "concurrently \"tsc\" \"npm run build-frontend\"",
        "copy-static-assets": "ts-node copyStaticAssets.ts",
        "debug": "npm run build && npm run watch-debug",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts,tsx}\" --quiet --fix",
        "serve": "node dist/server.js",
        "serve-debug": "nodemon --inspect dist/server.js",
        "start": "npm run serve",
        "test": "jest --forceExit --coverage --verbose",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
        "watch-node": "nodemon dist/server.js",
        "watch-test": "npm run test -- --watchAll",
        "watch-ts": "tsc -w"
    },
    "dependencies": {
        "async": "^3.1.0",
        "bcrypt-nodejs": "^0.0.3",
        "bluebird": "^3.5.5",
        "dotenv": "^8.2.0",
        "koa": "^2.13.0",
        "koa-compress": "^5.0.1",
        "koa-router": "^10.0.0",
        "koa-static": "^5.0.0",
        "less": "^4.0.0",
        "lodash": "^4.17.19",
        "mime": "^2.4.7",
        "nodemailer": "^6.3.0",
        "pug": "^2.0.4",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "utf8-byte-length": "^1.0.4",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/async": "^3.0.2",
        "@types/bcrypt-nodejs": "^0.0.30",
        "@types/bluebird": "^3.5.27",
        "@types/concurrently": "^4.1.0",
        "@types/eslint": "^6.1.1",
        "@types/jest": "^24.9.1",
        "@types/koa": "^2.11.6",
        "@types/koa-compress": "^4.0.1",
        "@types/koa-router": "^7.4.1",
        "@types/koa-static": "^4.0.1",
        "@types/less": "^3.0.2",
        "@types/lodash": "^4.14.141",
        "@types/node": "^12.19.11",
        "@types/nodemailer": "^6.2.1",
        "@types/pug": "^2.0.4",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/shelljs": "^0.8.5",
        "@types/tapable": "1.0.0",
        "@types/webpack": "^4.41.25",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^2.3.1",
        "@typescript-eslint/parser": "^2.3.1",
        "awesome-typescript-loader": "^5.2.1",
        "concurrently": "^5.3.0",
        "eslint": "^6.4.0",
        "eslint-plugin-react": "^7.22.0",
        "jest": "^24.9.0",
        "nodemon": "^1.19.2",
        "shelljs": "^0.8.3",
        "tapable": "1.0.0",
        "ts-jest": "^24.1.0",
        "ts-loader": "^8.0.14",
        "ts-node": "^8.4.1",
        "typescript": "^3.6.3",
        "webpack": "^5.11.1"
    }
}
