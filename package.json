{
  "name": "yuu-log",
  "version": "1.0.0",
  "description": "best blog system for myself",
  "keywords": [
    "typescript",
    "web",
    "boilerplate",
    "http2",
    "koa",
    "server push",
    "pug",
    "less",
    "webpack"
