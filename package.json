    "mime": "^2.4.6"
  },
  "devDependencies": {
    "@types/koa": "^2.11.6",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-router": "^7.4.1",
    "@types/koa-static": "^4.0.1",
    "@types/markdown-it": "^10.0.3",
    "@types/node": "^14.14.6",
    "commitizen": "^4.2.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "engines": {
